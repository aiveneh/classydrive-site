{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "package.json",
      "package-lock.json",
      "tsconfig.json",
      "tslint.json",
      "lib/*generated.js"
    ]
  },
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "ordered-imports":false
  },
  "rulesDirectory": []
}